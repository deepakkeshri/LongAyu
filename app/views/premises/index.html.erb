<h1>Listing premises</h1>

<table>
  <thead>
    <tr>
      <th>Parking</th>
      <th>Parking description</th>
      <th>Accessible to disabled</th>
      <th>Accessible to disabled description</th>
      <th>Public transport access</th>
      <th>Public transport access description</th>
      <th>Access without steps</th>
      <th>Access without steps description</th>
      <th>Disabled parking</th>
      <th>Disabled parking description</th>
      <th>Patient bathroom</th>
      <th>Patient bathroom description</th>
      <th>Wireless access</th>
      <th>Wireless access description</th>
      <th>Onsite pharmacy</th>
      <th>Onsite pharmacy description</th>
      <th>Centre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @premises.each do |premise| %>
      <tr>
        <td><%= premise.parking %></td>
        <td><%= premise.parking_description %></td>
        <td><%= premise.accessible_to_disabled %></td>
        <td><%= premise.accessible_to_disabled_description %></td>
        <td><%= premise.public_transport_access %></td>
        <td><%= premise.public_transport_access_description %></td>
        <td><%= premise.access_without_steps %></td>
        <td><%= premise.access_without_steps_description %></td>
        <td><%= premise.disabled_parking %></td>
        <td><%= premise.disabled_parking_description %></td>
        <td><%= premise.patient_bathroom %></td>
        <td><%= premise.patient_bathroom_description %></td>
        <td><%= premise.wireless_access %></td>
        <td><%= premise.wireless_access_description %></td>
        <td><%= premise.onsite_pharmacy %></td>
        <td><%= premise.onsite_pharmacy_description %></td>
        <td><%= premise.centre_id %></td>
        <td><%= link_to 'Show', premise %></td>
        <td><%= link_to 'Edit', edit_premise_path(premise) %></td>
        <td><%= link_to 'Destroy', premise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Premise', new_premise_path %>
